[
    {
        "type": "avsnitt",
        "style": "normaltekst",
        "children": [
            {
                "type": "span",
                "text": "Foreldrepengene dine regnes vanligvis ut fra gjennomsnittet av inntekten du har hatt i de"
            },
            {
                "type": "span",
                "text": "tre siste kalendermånedene",
                "marks": ["bold"]
            },
            {
                "type": "span",
                "text": "før foreldrepengeperioden din starter."
            }
        ]
    },
    {
        "type": "avsnitt",
        "style": "normaltekst",
        "children": [
            {
                "type": "span",
                "text": "Hvis det er mer enn "
            },
            {
                "type": "span",
                "text": "25 prosent forskjell ",
                "marks": ["bold"]
            },
            {
                "type": "span",
                "text": "mellom årsinntekten din og inntekten din de "
            },
            {
                "type": "span",
                "text": "tre siste månedene",
                "marks": ["bold"]
            },
            {
                "type": "span",
                "text": ", må vi vurdere hvilken inntekt som er mest representativ i din situasjon."
            }
        ]
    },
    {
        "type": " avsnitt",
        "style": "normaltekst",
        "children": [
            {
                "type": "span",
                "text": "Hvis du har jobbet så kort tid at det ikke er rapportert frilansinntekt for tre hele måneder til a-ordningen, skal inntekten for dette kortere tidsrommet brukes i beregningen."
            }
        ]
    },
    {
        "type": "avsnitt",
        "style": "normaltekst",
        "children": [
            {
                "type": "span",
                "text": "Hvis du både er "
            },
            {
                "type": "span",
                "text": "arbeidstaker og frilanser ",
                "marks": ["bold"]
            },
            {
                "type": "span",
                "text": "skal inntektene beregnes hver for seg og legges sammen til "
            },
            {
                "type": "span",
                "text": "ett beløp",
                "marks": ["bold"]
            },
            {
                "type": "span",
                "text": ". Hvis du har en samlet inntekt over "
            },
            {
                "type": "span",
                "text": "seks ganger grunnbeløpet",
                "marks": ["url_grunnbelopet"]
            },
            {
                "type": "span",
                "text": ", er det inntekten din som arbevidstaker som legges i bunnen av beregningen."
            }
        ],
        "markDefs": [
            {
                "key": "url_grunnbelopet",
                "type": "link",
                "href": "https://www.nav.no/383506/grunnbel%C3%B8pet-i-folketrygden"
            }
        ]
    },
    {
        "type": "avsnitt",
        "style": "normaltekst",
        "children": [
            {
                "type": "span",
                "text": "Hvis du både er "
            },
            {
                "type": "span",
                "text": "selvstendig næringsdrivende og frilanser eller arbeidstaker",
                "marks": ["bold"]
            },
            {
                "type": "span",
                "text": ", blir foreldrepengene dine beregnet ut fra "
            },
            {
                "type": "span",
                "text": "begge inntektene dine",
                "marks": ["bold"]
            },
            {
                "type": "span",
                "text": ". Inntekten din som frilanser skal alltid ligge i bunnen for beregningen. Frilanserinntekten opp til seks ganger "
            },
            {
                "type": "span",
                "text": "grunnbeløpet",
                "marks": ["url_grunnbeløp", "url_external"]
            },
            {
                "type": "span",
                "text": " dekkes alltid fullt ut."
            }
        ],
        "markDefs": [
            {
                "key": "url_grunnbeløp",
                "type": "link",
                "href": "https://www.nav.no/383506/grunnbel%C3%B8pet-i-folketrygden"
            }
        ]
    }
]
